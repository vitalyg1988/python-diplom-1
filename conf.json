 {
"version": 1,
"handlers": {
"fileHandler":{
  "class": "logging.handlers.RotatingFileHandler",
  "formatter": "formatterForLog",
  "filename": "logFile.log"
  }
},

"loggers":{
  "errorLog":{
    "handlers": ["fileHandler"],
    "level": "ERROR"
    }
  },

  "formatters": {
    "formatterForLog":{
      "format": "%(asctime)s - %(levelname)s - %(message)s"
    }
  }
}
